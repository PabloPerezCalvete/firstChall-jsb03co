let n1 = 5;
let n2 = 7;

let i = prompt();
console.log(i);
if (i == 1) {
  console.log(n1 + n2);
} else if (i == 2) {
  console.log(n1 - n2);
} else if (i == 3) {
  console.log(n1 * n2);
} else if (i == 4) {
  console.log(n1 / n2);
} else {
  console.log('error');
}
