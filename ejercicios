class persona {
  constructor(name, age, gender) {
    this.name = name;
    this.age = age;
    this.gender = gender;
  }
}
class teacher extends persona {
  constructor(subject, studentlist) {
    this.subject = subject;
    this.studentlist = studentlist;
    super(teacher.name);
  }
}
class student extends persona {
  constructor(course, group) {}
}
